  "dependencies": {
    "@prisma/client": "^4.2.1",
    "@types/koa__router": "^8.0.11",
    "bcrypt": "^5.0.1",
    "koa": "^2.13.4",
    "koa-body": "^5.0.0",
    "koa-convert": "^2.0.0",
    "koa-morgan": "^1.0.1",
    "koa-router": "^12.0.0",
    "koa-session": "^6.2.0",
    "pg": "^8.8.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^28.1.8",
    "@types/koa-convert": "^1.2.4",
    "@types/koa-morgan": "^1.0.5",
    "@types/koa-router": "^7.4.4",
    "@types/koa-session": "^5.10.6",
    "@types/node": "^18.7.13",
    "@types/supertest": "^2.0.12",
    "jest": "^27.0.0",
    "jest-mock-extended": "^2.0.4",
    "prisma": "^4.2.1",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.8"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "tsx"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{ts,js}",
      "!**/node_modules/**",
      "!**/build/**",
      "!**/coverage/**"
    ],
    "transform": {
      "\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "coverageReporters": [
      "text",
      "text-summary"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js|ts)x?$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/build/",
      "/coverage/"
    ]
  }
}
